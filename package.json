{
  "name": "milestone",
  "version": "0.0.1",
  "description": "Milestone chatbot sdk",
  "main": "dist/umd/milestone.js",
  "module": "dist/esm/milestone.js",
  "types": "dist/esm/Milestone.d.ts",
  "type": "module",
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "dev": "npx serve -p 8081",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "prepare": "husky install",
    "test": "npx jest",
    "coverage": "npx jest --coverage",
    "format": "prettier --write . && eslint --fix src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abhay-keyvalue/chatbot-js-poc"
  },
  "keywords": [
    "js",
    "chatbot",
    "sdk"
  ],
  "author": "KeyValue Software Systems",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abhay-keyvalue/chatbot-js-poc/issues"
  },
  "homepage": "https://github.com/abhay-keyvalue/chatbot-js-poc#readme",
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "eslint-plugin-compat": "^6.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-preact": "^0.1.0",
    "eslint-plugin-react": "^7.37.2",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "rollup": "^4.12.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-serve": "^1.1.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "5.5.3"
  },
  "dependencies": {
    "@microsoft/fetch-event-source": "^2.0.1",
    "preact": "^10.24.3",
    "promise-polyfill": "^8.3.0",
    "tslib": "^2.6.2",
    "whatwg-fetch": "^3.6.20"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint 'src/**/*.{ts,tsx}' --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "files": [
    "./dist",
    "license",
    "readme",
    "changelog"
  ]
}
